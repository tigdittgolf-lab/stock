		<queryString language="SQL">
			<![CDATA[SELECT fa.id AS "N°Ordre", fa.Narticle AS "Code Art.", art.designation AS "Designation", fa.Qte AS "Qté", fa.tva AS "TVA", fa.prix AS "Prix Unit.", fa.total_ligne AS "Total"
