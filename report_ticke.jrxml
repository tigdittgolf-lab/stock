	<queryString language="SQL">
		<![CDATA[SELECT
     bl.`NFact` AS bl_NFact,
     bl.`Nclient` AS bl_Nclient,
     bl.`date_fact` AS bl_date_fact,
     bl.`montant_ht` AS bl_montant_ht,
     bl.`timbre` AS bl_timbre,
     bl.`TVA` AS bl_TVA,
     bl.`autre_taxe` AS bl_autre_taxe,
     detail_bl.`Narticle` AS detail_bl_Narticle,
     detail_bl.`Qte` AS detail_bl_Qte,
     detail_bl.`tva` AS detail_bl_tva,
     detail_bl.`pr_achat` AS detail_bl_pr_achat,
     detail_bl.`prix` AS detail_bl_prix,
     detail_bl.`total_ligne` AS detail_bl_total_ligne,
     client.`Raison_sociale` AS client_Raison_sociale
FROM
     `detail_bl` detail_bl INNER JOIN `bl` bl ON detail_bl.`NFact` = bl.`NFact`
     INNER JOIN `client` client ON bl.`Nclient` = client.`Nclient`
WHERE
     bl.`Nfact` = null]]>
	</queryString>
